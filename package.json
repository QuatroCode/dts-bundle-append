  },
  "devDependencies": {
    "@types/yargs": "0.0.28",
    "@types/glob": "^5.0.29",
    "@types/node": "^6.0.31",
    "typescript": "^2.0.0"
  },
  "dependencies": {
    "glob": "^7.0.5",
